<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    
	<TextView 
	    android:id="@+id/case_browser_state"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    />
	<Button 
	    android:id="@+id/case_browser_redo"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:text="@string/case_bluetooth_redo"
	    android:layout_centerInParent="true"
	    />
	<Button
	    android:id="@+id/case_browser_wifi_setting"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:text="@string/case_browser_wifi_setting"
	    android:layout_below="@+id/case_browser_redo"
	    android:layout_centerHorizontal="true"
	    android:layout_marginTop="5dip"
	   	/>
</RelativeLayout>
